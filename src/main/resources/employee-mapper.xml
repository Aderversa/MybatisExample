<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twoshou.mapper.EmployeeMapper">
    <select id="selectAll" resultType="com.twoshou.pojo.Employee">
        select * from studyqt.employee
    </select>

    <select id="selectById" resultType="com.twoshou.pojo.Employee">
        select * from studyqt.employee where id = #{id}
    </select>
</mapper>